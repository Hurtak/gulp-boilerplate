{
  "name": "hurtak-frontend-boilerplate",
  "version": "2.0.0",
  "description": "all you need for develompent/compilation of simple frontend application",
  "scripts": {
    "clean": "rimraf ./dist",
    "scripts": "webpack --progress --config build/webpack.config.js",
    "styles": "lessc --verbose ./app/styles/styles.less ./dist/styles/styles.css",
    "styles:dist": "lessc --verbose --source-map-map-inline ./app/styles/styles.less | cleancss -o ./dist/styles/styles.css",
    "styles:hash": "hasha --algorithm=sha1 ./dist/styles/styles.css"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "6.5.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-regenerator": "6.4.4",
    "babel-plugin-transform-runtime": "6.4.3",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.5.0",
    "babelify": "7.2.0",
    "browser-sync": "2.10.1",
    "clean-css": "3.4.10",
    "gulp": "3.9.0",
    "gulp-autoprefixer": "3.1.0",
    "gulp-babel": "6.1.1",
    "gulp-flatten": "0.2.0",
    "gulp-htmlmin": "1.3.0",
    "gulp-if": "2.0.0",
    "gulp-less": "3.0.5",
    "gulp-load-plugins": "1.1.0",
    "gulp-minify-css": "1.2.2",
    "gulp-rev": "6.0.1",
    "gulp-rev-manifest-replace": "0.0.4",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "1.5.1",
    "gulp-util": "3.0.7",
    "hasha-cli": "^2.0.0",
    "normalize.css": "3.0.3",
    "rimraf": "2.5.0",
    "run-sequence": "1.1.5",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.6.1",
    "webpack": "1.12.14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hurtak/gulp-boilerplate"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "author": "Petr Huřťák (petr.hurtak@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hurtak/gulp-boilerplate/issues"
  },
  "homepage": "https://github.com/hurtak/gulp-boilerplate"
}
